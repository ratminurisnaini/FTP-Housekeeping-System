    <entry>
      <name>Delete FTP Folder</name>
      <description/>
      <type>EVAL</type>
      <attributes/>
      <script>var ftp = new Packages.org.apache.commons.net.ftp.FTPClient();
var ip_address = parent_job.getVariable("IP_Address");
var server_port = parent_job.getVariable("Server_Port");
var username = parent_job.getVariable("Username");
var password = parent_job.getVariable("Password");
var path = parent_job.getVariable("Remote_directory");
var folder_name = parent_job.getVariable("deleted_list");

// Create Function to Delete Folders
function deleteFolder(path) {
  var files = ftp.listFiles(path);
  for (var i = 0; i &lt; files.length; i++) {
    var file = files[i];
    var filePath = path + "/" + file.getName();
    if (file.isFile()) {
      ftp.deleteFile(filePath);
    } else if (file.isDirectory()) {
      deleteFolder(filePath);
    }
  }
  
  ftp.removeDirectory(path);
}

// Connect to FTP
ftp.connect(ip_address, server_port);
ftp.login(username, password);

// Delete Files
var folder_path = path + "/" + folder_name;
deleteFolder(folder_path);

ftp.logout();
ftp.disconnect();

result = true;</script>
