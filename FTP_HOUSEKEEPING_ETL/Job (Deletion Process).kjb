    <entry>
      <name>Get FTP Files/Folders</name>
      <description/>
      <type>EVAL</type>
      <attributes/>
      <script>// Get All Required Variables
var ftp = new Packages.org.apache.commons.net.ftp.FTPClient();
var ip_address = parent_job.getVariable("IP_Address");
var server_port = parent_job.getVariable("Server_Port");
var username = parent_job.getVariable("Username");
var password = parent_job.getVariable("Password");
var path = parent_job.getVariable("Remote_directory");
var dte_format = parent_job.getVariable("date_format");
var flag_path = parent_job.getVariable("flag_ftp");

var tanggal_reference = parent_job.getVariable("tanggal_reference");
var tanggal_reference_split = tanggal_reference.match(/(20\d{2})-(0[1-9]|1[0-2])-(0[1-9]|[12]\d|3[01])/);
var tahun_reference = tanggal_reference_split !== null ? parseInt(tanggal_reference_split[1], 10) : null;
var bulan_reference = tanggal_reference_split !== null ? (parseInt(tanggal_reference_split[2], 10)-1) : null;
var hari_reference = tanggal_reference_split !== null ? parseInt(tanggal_reference_split[3], 10) : null;
var tanggal_reference = new Date(tahun_reference, bulan_reference, hari_reference);

var list = [];

// Create Pattern
if (dte_format == "yyyymmdd"){
	var tanggal = "(20\\d{2})(0[1-9]|1[0-2])(0[1-9]|[12]\\d|3[01])";
} else if (dte_format == "yyyy-mm-dd"){
	var tanggal = "(20\\d{2})-(0[1-9]|1[0-2])-(0[1-9]|[12]\\d|3[01])";
} else if (dte_format == "ddmmyyyy"){
	var tanggal = "(0[1-9]|[12]\\d|3[01])(0[1-9]|1[0-2])(20\\d{2})";
} else if (dte_format == "dd-mm-yyyy"){
	var tanggal = "(0[1-9]|[12]\\d|3[01])-(0[1-9]|1[0-2])-(20\\d{2})";
} else if (dte_format == "yyyymm"){
	var tanggal = "(20\\d{2})(0[1-9]|1[0-2])";
} else if (dte_format == "yyyy-mm"){
	var tanggal = "(20\\d{2})-(0[1-9]|1[0-2])";
} else if (dte_format == "mmyyyy"){
	var tanggal = "(0[1-9]|1[0-2])(20\\d{2})";
} else if (dte_format == "mm-yyyy"){
	var tanggal = "(0[1-9]|1[0-2])-(20\\d{2})";
}

// Connect to FTP
ftp.connect(ip_address, server_port);
ftp.login(username, password);

// Delete Files
var objects = ftp.listFiles(path);
for (var i = 0; i &lt; objects.length; i++) {
    var object = objects[i];
    
	var cek = null;
	var hari = null;
	var bulan = null;
	var tahun = null;
	var tanggal_file_folder = null;
	
	pattern = new RegExp(tanggal);
	cek = object.getName().match(pattern);

	if (cek !== null){
		if (dte_format == "yyyymmdd" || dte_format == "yyyy-mm-dd"){
			hari = parseInt(cek[3], 10);
			bulan = parseInt(cek[2], 10)-1;
			tahun = parseInt(cek[1], 10);
		} else if (dte_format == "ddmmyyyy" || dte_format == "dd-mm-yyyy"){
			hari = parseInt(cek[1], 10);
			bulan = parseInt(cek[2], 10)-1;
			tahun = parseInt(cek[3], 10);
		} else if (dte_format == "yyyymm" || dte_format == "yyyy-mm"){
			hari = 1;
			bulan = parseInt(cek[2], 10)-1;
			tahun = parseInt(cek[1], 10);
		} else if (dte_format == "mmyyyy" || dte_format == "mm-yyyy"){
			hari = 1;
			bulan = parseInt(cek[1], 10)-1;
			tahun = parseInt(cek[2], 10);
		}
	}	
	
	tanggal_file_folder = new Date(tahun, bulan, hari);

	if (flag_path == "file" &amp;&amp; object.isFile() &amp;&amp; cek !== null &amp;&amp; tanggal_file_folder &lt; tanggal_reference){
		var file_name = object.getName();
		list.push(file_name);
	}
	else if (flag_path == "folder" &amp;&amp; object.isDirectory() &amp;&amp; cek !== null &amp;&amp; tanggal_file_folder &lt; tanggal_reference){
		var folder_name = object.getName();
		list.push(folder_name);
	}
}

ftp.logout();
ftp.disconnect();

parent_job.setVariable("tanggal", tanggal);
parent_job.setVariable("list", list);
parent_job.setVariable("output", tanggal_reference);
result = true;</script>
